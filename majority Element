class Solution {
    public int majorityElement(int[] nums) {
      int count = 0;
      int newValue = 0;

      for(int num : nums) {
        if(count == 0){
            newValue = num;
        }
        if(newValue == num){
            count++;
        }else{
            count--;
        }
      } 
      return newValue;
    }
}
